    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "dev": "echo 'Starting Vite development server...' && vite",
    "build": "vite build --mode production",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "analyze": "yarn build && npx vite-bundle-analyzer dist/assets/*.js",
    "audit": "yarn audit --json",
    "audit:fix": "yarn audit --fix",
    "security": "yarn audit && yarn outdated",
    "deps:update": "yarn upgrade-interactive",
    "deps:check": "yarn outdated",
    "clean": "rm -rf dist build coverage .nyc_output",
    "validate": "yarn type-check && yarn lint && yarn test:run",
    "ci": "yarn install --frozen-lockfile && yarn validate && yarn build",
    "start": "yarn dev",
    "serve": "yarn preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.9.2"
}
